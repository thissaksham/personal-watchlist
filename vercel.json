{
    "rewrites": [
        {
            "source": "/api/tmdb/(.*)",
            "destination": "/api/proxy?path=$1"
        },
        {
            "source": "/api/rapid/(.*)",
            "destination": "/api/rapid_proxy?path=$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}